{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        },
        {
            "src": "pages/index.html",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/",
            "dest": "pages/index.html"
        },
        {
            "src": "(.*)",
            "dest": "server.js"
        }
    ]
}
